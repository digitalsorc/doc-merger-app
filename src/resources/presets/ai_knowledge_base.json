{
  "header_style": "## {name}",
  "include_file_path": true,
  "include_doc_index": true,
  "separator_style": "---",
  "separator_blank_lines": 2,
  "generate_toc": true,
  "toc_depth": 2,
  "toc_style": "links",
  "toc_position": "top",
  "adjust_header_level": 0,
  "fix_relative_links": false,
  "strip_front_matter": true,
  "normalize_whitespace": true,
  "max_consecutive_blanks": 2,
  "detect_duplicates": true,
  "add_metadata": true,
  "add_semantic_markers": true,
  "add_chunk_hints": true,
  "extract_keywords": true,
  "include_patterns": ["*.md", "*.markdown"],
  "exclude_patterns": [],
  "recursive": true,
  "max_depth": -1,
  "sort_order": "natural",
  "sort_ascending": true,
  "output_encoding": "utf-8",
  "line_ending": "lf",
  "create_backup": true
}
